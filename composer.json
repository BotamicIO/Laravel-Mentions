{
    "name": "artisanry/mentions",
    "description": "Mentions based on Eloquent models for Laravel 5.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-mentions",
        "Laravel Mentions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "laravelcollective/html": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Mentions\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Mentions\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Mentions\\MentionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
